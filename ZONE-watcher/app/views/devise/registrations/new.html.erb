<% if session["devise.twitter_data"] != nil %>
<%= render 'new_twitter'%> 
<% elsif session["devise.github_data"] != nil%>
<%= render 'new_github'%>
<% elsif session["devise.google_data"] != nil%>
<%= render 'new_google'%>
<%else%>

<h2><%= raw t 'menu.signUp'%></h2>

<%= bootstrap_form_for(resource, :as => resource_name, :url => registration_path(resource_name),  :html => {:class => 'form-horizontal'}, help: :block ) do |f| %>
  <%= f.email_field :email %>
  <%= f.password_field :password %>
  <%= f.password_field :password_confirmation %>
  <%= f.control_group :join_mailing_list do %>
    <%= f.check_box :join_mailing_list, label: t('activerecord.attributes.user.join_mailing_list') %>
  <% end %>
  <%= f.actions do %>
    <%= f.primary raw t 'menu.signUp', :class=>'btn' %>
  <%end%>
<% end %>

<%= render "devise/shared/links" %>

<%end%>
