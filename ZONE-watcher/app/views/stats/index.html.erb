<table class ="table table-condensed table-striped table-hover ">
  <thead>
  <tr>
    <th>kind</th>
    <th>value</th>
  </tr>
  </thead>
<% @stats.each do |entry| %>
  <tr>
    <td><u><%= entry[:kind] %></u></td>
    <td><b><%= entry[:val] %></b></td>
  </tr>
<% end %>
</table>
